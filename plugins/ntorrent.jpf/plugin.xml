<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="ntorrent.jpf" version="0.1">
	<requires>
		<import plugin-id="ntorrent" />
		<import plugin-id="ntorrent.env" />
	</requires>
	<runtime>
		<library id="src" path="classes/" type="code">
			<export prefix="*" />
		</library>
	</runtime>
	<extension-point id="HandledPlugin">
		<parameter-def id="java-spec" type="number" default-value="1.5"/>
	</extension-point>
</plugin>