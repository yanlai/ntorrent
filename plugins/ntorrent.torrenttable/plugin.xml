<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="ntorrent.torrenttable" version="0.1">
	<requires>
		<import plugin-id="redstone.xmlrpc" />
		<import plugin-id="ntorrent.profile" />
		<import plugin-id="ntorrent.io.xmlrpc" />
		<import plugin-id="ntorrent.env" />
		<import plugin-id="ntorrent.viewmenu" />
	</requires>
	<runtime>
		<library id="src" path="classes/" type="code">
			<export prefix="*" />
		</library>
		<library id="icons" path="icons/" type="resources">
			<export prefix="*" />
		</library>		
	</runtime>
	<extension-point id="TorrentTableSorter" />
	<extension-point id="TorrentTablePopupMenu" />
	<extension-point id="TorrentSelectionListener" />
</plugin>
