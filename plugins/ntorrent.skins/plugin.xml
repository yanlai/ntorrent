<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="ntorrent.skins" version="0.1" class="ntorrent.skins.LookAndFeelHandler">
	<requires>
		<import plugin-id="ntorrent" />
		<import plugin-id="ntorrent.jpf" />
		<import plugin-id="ntorrent.locale" />
	</requires>
	<runtime>
		<library id="src" path="classes/" type="code">
			<export prefix="*" />
		</library>
		<library id="locale" path="locale/" type="resources"/>
	</runtime>
	<extension plugin-id="ntorrent.jpf" point-id="HandledPlugin" id="ntorrent.skins">
		<parameter id="java-spec" value="1.5" />
		<parameter id="name" value="Skins" />
	</extension>
</plugin>