<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="ntorrent.torrentlabels" version="0.1" class="ntorrent.torrentlabels.LabelController">
	<requires>
		<import plugin-id="ntorrent.torrenttable.sorter" />
		<import plugin-id="ntorrent.jpf" />
	</requires>
	<runtime>
		<library id="src" path="classes/" type="code">
			<export prefix="*" />
		</library>
	</runtime>
	<extension plugin-id="ntorrent.torrenttable.sorter" point-id="FilterExtension" id="ntorrent.torrentlabels@FilterExtension" />
	<extension plugin-id="ntorrent.jpf" point-id="HandledPlugin" id="ntorrent.torrentlabels@HandledPlugin">
		<parameter id="java-spec" value="1.6" />
	</extension>
</plugin>
